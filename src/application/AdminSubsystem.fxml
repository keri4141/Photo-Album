<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="application.AdminController">
   <children>
      <AnchorPane prefHeight="470.0" prefWidth="535.0">
         <children>
            <Label layoutX="210.0" layoutY="7.0" prefHeight="14.0" prefWidth="116.0" text="Logged in as Admin">
               <font>
                  <Font size="12.0" />
               </font>
            </Label>
            <ListView fx:id="userlist" layoutX="28.0" layoutY="71.0" prefHeight="265.0" prefWidth="479.0" />
            <Label layoutX="28.0" layoutY="34.0" text="Users">
               <font>
                  <Font size="19.0" />
               </font>
            </Label>
            <Button fx:id="userCreate" onAction="#handleCreate" layoutX="317.0" layoutY="365.0" mnemonicParsing="false" text="Create" />
            <Button fx:id="userDelete" onAction="#handleDelete" layoutX="399.0" layoutY="365.0" mnemonicParsing="false" text="Delete" />
            <TextField fx:id="usernameField" layoutX="90.0" layoutY="365.0" promptText="Username" />
            <Button fx:id="Logout" onAction="#handleLogout" layoutX="446.0" layoutY="33.0" mnemonicParsing="false" text="Logout" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
